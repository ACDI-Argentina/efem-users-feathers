  "scripts": {
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint src/. test/.",
    "eslint:fix": "eslint src/. test/. --fix",
    "deploy:ganache": "NODE_ENV=ganache node scripts/deployProcess.js",
    "deploy:rsk": "NODE_ENV=rsk node scripts/deployProcess.js",
    "deploy:rsk_testnet": "NODE_ENV=rsk_testnet node scripts/deployProcess.js",
    "deploy:rsk_mainnet": "NODE_ENV=rsk_mainnet node scripts/deployProcess.js",
    "start": "babel-watch ./src/index.js",
    "start:rsk": "NODE_ENV=rsk babel-watch ./src/index.js",
    "start:ganache": "NODE_ENV=ganache babel-watch ./src/index.js",
    "start:rsk_testnet": "NODE_ENV=rsk_testnet babel-watch ./src/index.js",
    "start:network:rsk": "NODE_ENV=rsk node scripts/network/startRSK.js",
    "start:network:rsk:debug": "NODE_ENV=rsk LOG_LEVEL=debug node scripts/network/startRSK.js",
    "start:network:ganache": "NODE_ENV=ganache node scripts/network/startGanache.js",
    "start:network:ganache:debug": "NODE_ENV=ganache LOG_LEVEL=debug node scripts/network/startGanache.js",
    "serve": "pm2 startOrRestart ecosystem.config.js --env production",
    "mocha": "mocha test/ --recursive",
    "prettify": "prettier --write 'src/**/*.js*'",
    "reset-db": "node scripts/resetDataBase.js",
    "reset-rsk-db": "node scripts/rsk/resetRSKDataBase.js",
    "mongo": "mongod --fork --syslog"
